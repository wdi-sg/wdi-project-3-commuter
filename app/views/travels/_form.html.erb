<%= form_for travel_data do |f| %>

<div class="field">
  <%= f.label :start_route, class: 'label' %>
  <%= f.text_field :start_route, class: 'input' %>
</div>

<div class="field">
  <%= f.label :end_route, class: 'label' %>
  <%= f.text_field :end_route, class: 'input' %>
</div>

<div class="field">
  <%= f.label :timeslot, class: 'label' %>
  <%= f.text_field :timeslot, class: 'input' %>
</div>

<%= f.submit %>
<% end %>
